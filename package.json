{
  "name": "wkx",
  "version": "0.5.0",
  "description": "A WKT/WKB/EWKT/EWKB/TWKB/GeoJSON parser and serializer",
  "type": "module",
  "main": "./dist/wkx.cjs.js",
  "module": "./dist/wkx.es.js",
  "browser": "./dist/wkx.umd.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/wkx.es.js",
      "require": "./dist/wkx.cjs.js",
      "default": "./dist/wkx.umd.js"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "coverage": "vitest run --coverage && coveralls < coverage/lcov.info",
    "prepare": "npm run build"
  },
  "author": "Christian Schwarz",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "async": "^3.2.6",
    "coveralls": "^3.1.1",
    "deep-eql": "^5.0.2",
    "esbuild": "^0.25.6",
    "eslint": "^9.31.0",
    "json-stringify-pretty-compact": "^4.0.0",
    "mkdirp": "^3.0.1",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cschwarz/wkx.git"
  },
  "keywords": [
    "wkt",
    "wkb",
    "ewkt",
    "ewkb",
    "twkb",
    "geojson",
    "ogc",
    "geometry",
    "geography",
    "spatial"
  ],
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
